{
  "name": "wowcup",
  "description": "The finest Worldcup 2018 CLI.",
  "version": "0.0.7",
  "author": "Hoang Nhat Nguyen @codeaholicguy",
  "bin": {
    "wowcup": "./bin/run"
  },
  "bugs": "https://github.com/codeaholicguy/wowcup/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/cli-table2": "^0.2.1",
    "axios": "^0.18.0",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "cli-table2": "^0.2.0",
    "cli-ux": "^4.6.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "node-emoji": "^1.8.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^1",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4",
    "@types/chalk": "^2.2.0",
    "@types/lodash": "^4.14.109",
    "@types/mocha": "^5",
    "@types/moment": "^2.13.0",
    "@types/node": "^10",
    "@types/node-emoji": "^1.8.0",
    "chai": "^4",
    "globby": "^8",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "mocha": "^5",
    "nyc": "^12",
    "ts-node": "^6",
    "tslint": "^5",
    "typescript": "^2.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/codeaholicguy/wowcup",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "wowcup",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "codeaholicguy/wowcup",
  "scripts": {
    "lint": "tslint -p test -t stylish",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "prepare": "rm -rf lib && tsc",
    "precommit": "lint-staged",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "lint-staged": {
    "*.ts": ["yarn run lint"]
  },
  "types": "lib/index.d.ts"
}
